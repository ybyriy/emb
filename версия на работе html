    <script>
       // Бургер-меню
        const burger = document.querySelector('.burger');
        const navUl = document.querySelector('nav ul');

        burger.addEventListener('click', () => {
            navUl.classList.toggle('show');
        });

        // Закрытие меню при клике на ссылку
        document.querySelectorAll('nav ul a').forEach(link => {
            link.addEventListener('click', () => {
                navUl.classList.remove('show');
            });
        });
   
        // Фильтрация товаров
        const filterLinks = document.querySelectorAll('.filter');
        const categories = document.querySelectorAll('.category');

        filterLinks.forEach(link => {
            link.addEventListener('click', function() {
                filterLinks.forEach(l => l.classList.remove('active'));
                this.classList.add('active');

                const filter = this.getAttribute('data-filter');

                categories.forEach(category => {
                    if (filter === 'all') {
                        category.classList.remove('hidden');
                    } else {
                        if (category.getAttribute('data-category') === filter) {
                            category.classList.remove('hidden');
                        } else {
                            category.classList.add('hidden');
                        }
                    }
                });
            });
        });

        // Слайдер для каждого образца
        document.querySelectorAll('.product-slider').forEach(sliderContainer => {
            const slider = sliderContainer.querySelector('.product-slides');
            const prevBtn = sliderContainer.querySelector('.prev');
            const nextBtn = sliderContainer.querySelector('.next');
            let currentIndex = 0;
            const slides = slider.querySelectorAll('img');
            const totalSlides = slides.length;

            function updateSlider() {
                slider.style.transform = `translateX(-${currentIndex * 100}%)`;
            }

            prevBtn.addEventListener('click', () => {
                currentIndex = (currentIndex > 0) ? currentIndex - 1 : totalSlides - 1;
                updateSlider();
            });

            nextBtn.addEventListener('click', () => {
                currentIndex = (currentIndex < totalSlides - 1) ? currentIndex + 1 : 0;
                updateSlider();
            });
        });

        // Модальное окно для кликабельных изображений
        const modal = document.getElementById('imageModal');
        const modalImg = modal.querySelector('img');
        const modalClose = modal.querySelector('.modal-close');
        const images = document.querySelectorAll('.product-slides img');

        images.forEach(img => {
            img.addEventListener('click', () => {
                modalImg.src = img.src;
                modalImg.alt = img.alt;
                modal.style.display = 'flex';
            });
        });

        modalClose.addEventListener('click', () => {
            modal.style.display = 'none';
        });

        // Закрытие модального окна при клике вне изображения
        modal.addEventListener('click', (e) => {
            if (e.target === modal) {
                modal.style.display = 'none';
            }
        });
    </script>
    <!--Телефонная трубка-->
<script>
window.addEventListener('DOMContentLoaded', function() {
    setTimeout(function(){
        document.getElementById('phone-fab').classList.add('visible');
    }, 6000); // появляется через 6 секунд
});
</script>
<script>
    const video = document.querySelector('.video_background');
    const videoSource = video.querySelector('source');
    const videos = [
        'video/IMG_2609.MOV' // Замените на фактические имена файлов
    ];
    let currentVideoIndex = 0;
    let shouldContinue = true;

    function playNextVideo() {
        if (currentVideoIndex < videos.length) {
            videoSource.src = videos[currentVideoIndex];    
            video.load();
            video.play().catch(error => {
                console.error('Error playing video:', error);
            });
            currentVideoIndex++;
        } else {
            shouldContinue = false;
            // Видео закончились, можно остановить проигрывание или выполнить другие действия
        }
    }

    // Функция сброса и запуска видео при навигации
    function resetAndPlayVideos() {
        currentVideoIndex = 0; // Сбрасываем индекс на начало списка
        shouldContinue = true; // Сбрасываем флаг
        playNextVideo(); // Запускаем воспроизведение первого видео
    }

    // Воспроизводим первое видео при загрузке страницы
    playNextVideo();

    // Переключаемся на следующее видео, когда текущее заканчивается
    video.addEventListener('ended', () => {
        if (shouldContinue) {
            playNextVideo();
        }
    });

    // Обеспечиваем воспроизведение видео на мобильных устройствах
    document.addEventListener('click', () => {
        if (video.paused && shouldContinue) {
            video.play().catch(error => {
                console.error('Error playing video on click:', error);
            });
        }
    });

    // --- НОВОЕ: Обработчики для навигации ---
    // Находим все элементы навигации, которые должны перезапускать видео
    const navLinks = document.querySelectorAll('.filter'); // Используйте ваш селектор

    navLinks.forEach(link => {
        link.addEventListener('click', (event) => {
            // Если вы используете SPA (Single Page Application) с предотвращением дефолтного поведения
            // event.preventDefault(); 
            
            resetAndPlayVideos();
            // Дополнительно можно выполнить вашу логику перехода по навигации здесь
        });
    });

</script>



</body>
